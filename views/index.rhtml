<script type="text/javascript">

  function checkForm(form)
  {
    // validation fails if the input is blank
    if(form.email.value == "" || form.lname.value == "" || form.fname.value == "") {
      alert("Email, Nombre y Apellido son datos requeridos para contactarnos!");
      form.email.focus();
      return false;
    }

    // regular expression to match names
    var re = /\S{2,}.*/;

    // validation fails if the input doesn't match our regular expression
    if(!re.test(form.lname.value)) {
      alert("Error: el apellido parece muy corto");
      form.lname.focus();
      return false;
    }
		if(!re.test(form.fname.value)) {
      alert("Error: el nombre parece muy corto");
      form.fname.focus();
      return false;
    }

    // validation was successful
    return true;
  }

</script>

