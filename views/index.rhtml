		<!-- CTA -->
			<section id="cta">

				<header>
					<h2>¿List@ para <strong>iniciar el cambio</strong>?</h2>
					<p></p>
				</header>
				<div class="row">
					<div class="3u"></div>
					<div class="6u 12u(narrower)">
						<%if @errormsg.to_s!='' && @errormsg!='OK'%>
							<div class="error">
					     <h3>Ups, ocurrió un error</h3>
					     <p><%= @errormsg%></p>
							</div>
						<%end%>
						<%if @errormsg=='OK'%>
							<div >
					     <h3>¡Enviado! Nos contactaremos contigo en breve.</h3>
							</div>
						<%end%>
						<form method="POST" action="/contacto/facilitacion" onsubmit="return checkForm(this);">
						<input type="email" name="email" class="suscription-textbox" placeholder="tu@correo.com" value="<%= @f[:email]%>" required/>
						<br/>
						<input type="text" name="fname" class="suscription-textbox" placeholder="Nombre" value="<%= @f[:fname]%>" required pattern="\S{3,}.*"/>
						<br/>
						<input type="text" name="lname" class="suscription-textbox" placeholder="Apellido" value="<%= @f[:lname]%>" required pattern="\S{3,}.*"/>
						<br/>
						<input type="text" name="zone" class="suscription-textbox" placeholder="Tu Ciudad" value="<%= @f[:zone]%>"/>
						<br/>
						<input type="text" name="interest" class="suscription-textbox" placeholder="¿Qué actividad te interesa?" value="<%= @f[:interest]%>"/>
					</form>
					</div>
				</div>
				<footer>
					<br/>
					<ul class="buttons">
						<li><a class="button vertical btn-kleer btn-warning" onclick="$('form').submit()">
							¡Contactanos!</a></li>
					</ul>
				</footer>

			</section>

